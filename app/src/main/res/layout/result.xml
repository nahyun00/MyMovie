<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_vertical"
    android:stretchColumns="1">

        <TableRow>
            <TextView
                android:id="@+id/tv1"
                android:layout_gravity="center_vertical"
                android:text="오징어게임"
                android:textSize="17dp" />

            <RatingBar
                android:id="@+id/rbar1"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"/>
        </TableRow>

        <TableRow>
            <TextView
                android:id="@+id/tv2"
                android:layout_gravity="center_vertical"
                android:text="지옥"
                android:textSize="17dp"/>
            <RatingBar
                android:id="@+id/rbar2"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"/>
        </TableRow>

        <TableRow>
            <TextView
                android:id="@+id/tv3"
                android:layout_gravity="center_vertical"
                android:text="스위트 홈"
                android:textSize="17dp" />
            <RatingBar
                android:id="@+id/rbar3"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"/>
        </TableRow>

        <TableRow>
            <TextView
                android:id="@+id/tv4"
                android:layout_gravity="center_vertical"
                android:text="마이네임"
                android:textSize="17dp" />
            <RatingBar
                android:id="@+id/rbar4"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"/>
        </TableRow>

        <TableRow>
            <TextView
                android:id="@+id/tv5"
                android:layout_gravity="center_vertical"
                android:text="킹덤"
                android:textSize="17dp" />
            <RatingBar
                android:id="@+id/rbar5"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"/>
        </TableRow>

        <TableRow>
            <TextView
                android:id="@+id/tv6"
                android:layout_gravity="center_vertical"
                android:text="D.P"
                android:textSize="17dp" />
            <RatingBar
                android:id="@+id/rbar6"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"/>
        </TableRow>

        <TableRow>
            <TextView
                android:id="@+id/tv7"
                android:layout_gravity="center_vertical"
                android:text="인간수업"
                android:textSize="17dp" />
            <RatingBar
                android:id="@+id/rbar7"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"/>
        </TableRow>

        <TableRow>
            <TextView
                android:id="@+id/tv8"
                android:layout_gravity="center_vertical"
                android:text="연모"
                android:textSize="17dp" />

            <RatingBar
                android:id="@+id/rbar8"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right" />

        </TableRow>

        <TableRow>
            <TextView
                android:id="@+id/tv9"
                android:layout_gravity="center_vertical"
                android:text="보건교사 안은영"
                android:textSize="17dp" />
            <RatingBar
                android:id="@+id/rbar9"
                style="?android:attr/ratingBarStyleIndicator"
                android:layout_gravity="right"/>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/btnReturn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:layout_span="2"
                android:text="돌아가기" />

        </TableRow>

    </TableLayout>
